{
  "name": "@bassist/changelog",
  "version": "0.2.0",
  "description": "Simple CHANGELOG generator by @chengpeiquan , based on conventional-changelog-cli.",
  "author": "chengpeiquan <chengpeiquan@chengpeiquan.com>",
  "license": "MIT",
  "homepage": "https://github.com/chengpeiquan/bassist/tree/main/packages/changelog",
  "files": [
    "dist"
  ],
  "bin": {
    "@bassist/changelog": "./dist/index.cjs",
    "changelog": "./dist/index.cjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chengpeiquan/bassist.git"
  },
  "keywords": [
    "changelog",
    "changelog generator",
    "generate changelog",
    "CHANGELOG.md"
  ],
  "dependencies": {
    "@withtypes/minimist": "^0.1.1",
    "conventional-changelog-cli": "^4.1.0"
  }
}