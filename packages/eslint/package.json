{
  "name": "@bassist/eslint",
  "version": "0.5.0",
  "description": "Flat ESLint config for JavaScript, TypeScript, Vue, React, Svelte, Prettier and so on by @chengpeiquan .",
  "author": "chengpeiquan <chengpeiquan@chengpeiquan.com>",
  "license": "MIT",
  "homepage": "https://github.com/chengpeiquan/bassist/tree/main/packages/eslint",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "svelte": [
        "./dist/svelte.d.ts"
      ],
      "*": [
        "./dist/index.d.ts",
        "./dist/*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./svelte": {
      "types": "./dist/svelte.d.ts",
      "import": "./dist/svelte.mjs",
      "require": "./dist/svelte.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chengpeiquan/bassist.git"
  },
  "keywords": [
    "eslint",
    "eslint config"
  ],
  "dependencies": {
    "@eslint/js": "^8.50.0",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "@unocss/eslint-plugin": "^0.56.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-define-config": "^1.23.0",
    "eslint-plugin-antfu": "^0.43.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-svelte": "^2.33.2",
    "eslint-plugin-unicorn": "^48.0.1",
    "eslint-plugin-vue": "^9.17.0",
    "globals": "^13.22.0",
    "prettier": "^3.0.3",
    "svelte-eslint-parser": "^0.33.0",
    "typescript": "^5.2.2",
    "vue-eslint-parser": "^9.3.1"
  },
  "devDependencies": {
    "svelte": "^4.2.1"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0",
    "svelte": ">=4.0.0",
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "svelte": {
      "optional": true
    }
  }
}