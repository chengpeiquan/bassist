{
  "name": "@bassist/eslint",
  "version": "0.6.0",
  "description": "Flat ESLint config for JavaScript, TypeScript, Vue, React, Svelte, Prettier and so on by @chengpeiquan .",
  "author": "chengpeiquan <chengpeiquan@chengpeiquan.com>",
  "license": "MIT",
  "homepage": "https://github.com/chengpeiquan/bassist/tree/main/packages/eslint",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "svelte": [
        "./dist/svelte.d.ts"
      ],
      "*": [
        "./dist/index.d.ts",
        "./dist/*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./svelte": {
      "types": "./dist/svelte.d.ts",
      "import": "./dist/svelte.mjs",
      "require": "./dist/svelte.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chengpeiquan/bassist.git"
  },
  "keywords": [
    "eslint",
    "eslint config"
  ],
  "dependencies": {
    "@eslint/js": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@unocss/eslint-plugin": "^0.58.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-antfu": "^2.1.2",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-svelte": "^2.35.1",
    "eslint-plugin-unicorn": "^51.0.1",
    "eslint-plugin-vue": "^9.21.1",
    "globals": "^14.0.0",
    "prettier": "^3.2.5",
    "svelte-eslint-parser": "^0.33.1",
    "typescript": "^5.3.3",
    "vue-eslint-parser": "^9.4.2"
  },
  "devDependencies": {
    "svelte": "^4.2.10"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0",
    "svelte": ">=4.0.0",
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "svelte": {
      "optional": true
    }
  }
}